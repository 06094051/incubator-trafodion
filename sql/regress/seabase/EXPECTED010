>>merge into t010t3
+>  using (select * from t010t2) as src
+>  on ((src.a, src.b, src.c) = (a,b,c))
+>when matched
+>  then update set d = src.d
+>when not matched
+>  then insert values (src.a, src.b, src.c, src.d)
+>;

