  fragment_id ............ 0
  parent_frag ............ (none)
  fragment_type .......... master
  iud_type ............... trafodion_update TRAFODION.SCH.T010T2
  new_rec_expr ........... (D assign %('uuu'))
  predicate .............. (C < 2) and ((B = 'a') or (B = 'b'))
  begin_key .............. (A = %(4)) and (B = '<min>') and (C = <min>)
  end_key ................ (A = %(4)) and (B = '<max>') and (C = 2)

